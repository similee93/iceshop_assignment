CREATE TABLE `images_db`.`tbl_images` ( `id` BIGINT NOT NULL AUTO_INCREMENT , `client_id` VARCHAR(255) NOT NULL , `original_name` VARCHAR(191) NOT NULL , `hash` VARCHAR(191) NOT NULL , `format` VARCHAR(6) NOT NULL , `size_bites` INT(20) NOT NULL , `url` VARCHAR(191) NOT NULL , `status` ENUM('success','processing','failed') NOT NULL DEFAULT 'processing' , PRIMARY KEY (`id`)) ENGINE = InnoDB;